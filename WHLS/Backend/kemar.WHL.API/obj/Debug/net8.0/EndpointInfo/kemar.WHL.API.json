            "nullable": true
          },
          "isActive": {
            "type": "boolean"
          },
          "distanceId": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "address": {
            "minLength": 3,
            "type": "string"
          },
          "city": {
            "minLength": 2,
            "type": "string"
          },
          "country": {
            "minLength": 2,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "ForgotPasswordRequest": {
        "type": "object",
