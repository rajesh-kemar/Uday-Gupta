          },
          "country": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "DistanceRequest": {
        "required": [
          "address",
          "city",
          "country"
        ],
        "type": "object",
        "properties": {
          "isDeleted": {
            "type": "boolean"
          },
          "createdBy": {
            "type": "string",
            "nullable": true
          },
          "updatedBy": {
            "type": "string",
            "nullable": true
          },
          "createdAt": {
